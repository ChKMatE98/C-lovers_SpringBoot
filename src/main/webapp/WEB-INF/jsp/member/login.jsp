<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="/css/member/login.css">
    <title>로그인</title>
</head>
<body class="login_body">
    <div class="container">
        <div class="loginBox">
            <div class="loginBox__inner">
                <div class="loginBox__logo">
                    <div class="logo">
                        <span>C-lover</span>
                    </div>
                </div>
                <div class="loginBox__saveBox">
                    <div class="loginBox__save">
                        <input class="saveBox" type="checkbox"><p>사번 저장</p>
                    </div>
                </div>
                <div class="loginBox__idBox">
                    <input class="idBox" type="text" placeholder="  사번">
                </div>
                <div class="loginBox__pwBox">
                    <input class="pwBox" type="password" placeholder="  비밀번호">
                </div>
                <div class="loginBox__loginBtn">
                    <button class="loginBtn">로그인</button>
                </div>
                <div class="loginBox__findPw">
                    <button class="findPwBtn">비밀번호 찾기</button>
                </div>
            </div>
        </div>
    </div>
</body>
</html>