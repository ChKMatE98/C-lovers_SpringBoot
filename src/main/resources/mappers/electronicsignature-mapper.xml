<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ElectronicSignature">
	<!-- 전자결재 매퍼 -->
	<!-- // 멤버의 전자 결재를 위한 전자선 정렬 -> job_id의 순서대로 정렬 -->
	<select id="selectEmpJobLevel" resultType="map">
		select *
		from emp_job_sec_level
		where id in
		<foreach item="item" collection="list" open="(" separator=","
			close=")">
			#{item}
		</foreach>
	</select>

	<insert id="insertVacation" useGeneratedKeys="true"
		keyProperty="id" parameterType="Map">
		<selectKey keyProperty="id" resultType="Integer"
			order="AFTER">
			SELECT LAST_INSERT_ID() AS id
		</selectKey>
		insert into vacation_document (emp_id) values (#{emp_id});
	</insert>
</mapper>